-- Run this code till the end to create all the tables and policies in one go. Then also craete a images public bucket in storage in supabse .

CREATE EXTENSION IF NOT EXISTS "uuid-ossp";  -- Enable UUID extension if not already enabled

CREATE TABLE public.users (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
    username VARCHAR(50) NOT NULL UNIQUE,
    email VARCHAR(100) UNIQUE NOT NULL,
    google_connected BOOLEAN NOT NULL DEFAULT FALSE,
    github_connected BOOLEAN NOT NULL DEFAULT FALSE,
    public_connected BOOLEAN NOT NULL DEFAULT FALSE,  -- maybe default Public is true if the user signs up via public registration
    created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE public.public_logins (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
    user_id UUID REFERENCES public.users(id) ON DELETE CASCADE,
    username VARCHAR(50) NOT NULL UNIQUE,
    email VARCHAR(100) UNIQUE NOT NULL,
    password TEXT NOT NULL,
    is_email_verified BOOLEAN DEFAULT FALSE,
    reset_password_token VARCHAR(255),
    created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE public.google_logins (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
    user_id UUID REFERENCES public.users(id) ON DELETE CASCADE,
    google_id VARCHAR(50) UNIQUE NOT NULL,
    display_name VARCHAR(100) NOT NULL,
    family_name VARCHAR(100),
    given_name VARCHAR(100),
    email VARCHAR(100) NOT NULL,
    picture TEXT,
    raw_json JSONB,
    created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE public.github_logins (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
    user_id UUID REFERENCES public.users(id) ON DELETE CASCADE,
    github_id VARCHAR(50) UNIQUE NOT NULL,
    display_name VARCHAR(100) NOT NULL,
    username VARCHAR(100),
    email VARCHAR(100),
    picture TEXT,
    raw_json JSONB,
    created_at TIMESTAMP DEFAULT NOW()
);

CREATE EXTENSION IF NOT EXISTS "uuid-ossp";  -- Enable UUID generation if not already enabled

CREATE TABLE public.manage_logins (
    id SERIAL PRIMARY KEY,
    user_id UUID REFERENCES public.users(id) ON DELETE CASCADE,
    session_id TEXT NOT NULL,      -- Unique identifier for this session (could be a random token)
    device_info JSONB,             -- Details like browser, OS, etc.
    method VARCHAR(50) NOT NULL CHECK (method IN ('public', 'google', 'github')), -- Authentication method
    auth_token TEXT NOT NULL,           -- Token issued for this auth token
    created_at TIMESTAMP DEFAULT NOW(),   -- When the session was initiated
    expires_at TIMESTAMP NOT NULL,          -- When the token expires
    ip_address TEXT,               -- Client IP address for logging/security
    is_logged_in BOOLEAN DEFAULT TRUE       -- True when the session is active; false when logged out or invalidated
);

CREATE TABLE public.sessions (
    id SERIAL PRIMARY KEY,
    session_id VARCHAR(128) NOT NULL,          -- Should match the session_id in the logins table
    session_token TEXT NOT NULL,           -- Token issued for this login session
    user_agent TEXT,
    language VARCHAR(32),
    platform VARCHAR(64),
    screen_resolution VARCHAR(32),
    timezone_offset INTEGER,
    generated_at BIGINT NOT NULL,              -- Timestamp (in ms) when token was generated
    last_access BIGINT,                        -- Timestamp (in ms) of last activity
    created_at TIMESTAMP DEFAULT NOW()         -- Record creation timestamp
);


CREATE TABLE public.signup_limits (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(), -- Unique ID for tracking signup attempts
    email TEXT, -- Email address being signed up
    ip_address TEXT NOT NULL, -- IP address of the requester
    attempts INT DEFAULT 0, -- Number of signup attempts
    last_attempt TIMESTAMP DEFAULT NOW(), -- When the last attempt was made
    created_at TIMESTAMP DEFAULT NOW(), -- Entry creation time
    CONSTRAINT unique_email_ip UNIQUE (email, ip_address) -- Prevent duplicate entries for the same email-IP pair
);

CREATE TABLE public.failed_login_attempts (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(), -- Unique ID for failed login attempt tracking
    email TEXT NOT NULL, -- Email address being logged in
    ip_address TEXT NOT NULL, -- IP address of the requester
    failed_attempts INT DEFAULT 0, -- Count of failed login attempts
    locked_until TIMESTAMP, -- If locked, the time until account is unlocked
    created_at TIMESTAMP DEFAULT NOW() -- Entry creation time
);

CREATE POLICY allow_insert_for_authenticated
ON public.manage_logins
FOR INSERT
TO authenticated
WITH CHECK (true);


-- Create the `otps` table
CREATE TABLE public.otps (
    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(), -- Auto-generate unique UUID for each OTP
    email TEXT NOT NULL, -- Email tied to the OTP
    otp TEXT NOT NULL, -- The one-time password
    purpose VARCHAR(50) NOT NULL CHECK (purpose IN ('password_reset', 'email_verification')), -- OTP purpose validation
    expires_at TIMESTAMP NOT NULL, -- Expiration timestamp for the OTP
    created_at TIMESTAMP DEFAULT NOW(), -- Automatically set to current timestamp
    CONSTRAINT unique_email_purpose UNIQUE (email, purpose) -- Ensure only one active OTP per purpose
);

CREATE INDEX otps_email_idx ON public.otps(email);
CREATE INDEX otps_expires_at_idx ON public.otps(expires_at);


CREATE TABLE profiles (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    user_id UUID UNIQUE REFERENCES users(id) ON DELETE CASCADE,
    username VARCHAR(100) UNIQUE NOT NULL,
    dp TEXT,
    name VARCHAR(255) NOT NULL,
    bio TEXT,
    followers_count INT DEFAULT 0,
    following_count INT DEFAULT 0,
    profile_views INT DEFAULT 0,
    social_links TEXT, -- Stores all social links in one field as JSON or CSV format
    requested_username VARCHAR(100) UNIQUE,
    username_change_requested_at TIMESTAMP,
    verified BOOLEAN DEFAULT FALSE, -- Indicates if a user is verified
    verified_at TIMESTAMP, -- Timestamp for verification approval
    created_at TIMESTAMP DEFAULT NOW()
);


CREATE TABLE verification_requests (
    id SERIAL PRIMARY KEY,
    user_id UUID UNIQUE REFERENCES profiles(user_id) ON DELETE CASCADE,
    request_status VARCHAR(20) DEFAULT 'pending', -- ['pending', 'approved', 'rejected']
    request_reason TEXT, -- User’s reason for requesting verification
    submitted_at TIMESTAMP DEFAULT NOW(),
    reviewed_at TIMESTAMP
);

CREATE TABLE wallpapers (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  user_id UUID REFERENCES users(id),
  title VARCHAR(255) NOT NULL,
  description TEXT,
  image_url TEXT NOT NULL,
  status VARCHAR(20) DEFAULT 'pending', -- e.g., 'pending', 'uploading', 'published', 'manual_approval'
  view_count INT DEFAULT 0,
  download_count INT DEFAULT 0,
  share_count INT DEFAULT 0,
  like_count INT DEFAULT 0,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE categories (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  name VARCHAR(100) UNIQUE NOT NULL,
  created_at TIMESTAMP DEFAULT NOW()
);

-- Junction table for many-to-many relationship between wallpapers and categories
CREATE TABLE wallpaper_categories (
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  category_id UUID REFERENCES categories(id) ON DELETE CASCADE,
  PRIMARY KEY (wallpaper_id, category_id)
);

CREATE TABLE category_thumbnails (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  category_id UUID REFERENCES categories(id) ON DELETE CASCADE,
  thumbnail_url TEXT NOT NULL,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE hashtags (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  name VARCHAR(100) UNIQUE NOT NULL,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE wallpaper_hashtags (
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  hashtag_id UUID REFERENCES hashtags(id) ON DELETE CASCADE,
  PRIMARY KEY (wallpaper_id, hashtag_id)
);

CREATE TABLE wallpaper_likes (
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  liked_at TIMESTAMP DEFAULT NOW(),
  created_at TIMESTAMP DEFAULT NOW(),  -- Added comma here
  PRIMARY KEY (wallpaper_id, user_id)
);

CREATE TABLE user_follows (
  follower_id UUID REFERENCES users(id) ON DELETE CASCADE,
  follower_profile_id UUID REFERENCES profiles(id),
  following_id UUID REFERENCES users(id) ON DELETE CASCADE,
  following_profile_id UUID REFERENCES profiles(id),
  created_at TIMESTAMP DEFAULT NOW(),
  PRIMARY KEY (follower_id, following_id)
);

-- Allow anyone to SELECT from storage.objects
CREATE POLICY "Allow_all_select" 
    ON storage.objects
    FOR SELECT
    TO public
    USING (true);

-- Allow anyone to INSERT into storage.objects
CREATE POLICY "Allow_all_insert"
    ON storage.objects
    FOR INSERT
    TO public
    WITH CHECK (true);

-- Allow anyone to UPDATE storage.objects
CREATE POLICY "Allow_all_update"
    ON storage.objects
    FOR UPDATE
    TO public
    USING (true);

-- Allow anyone to DELETE from storage.objects
CREATE POLICY "Allow_all_delete"
    ON storage.objects
    FOR DELETE
    TO public
    USING (true);

CREATE TABLE reports (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  reporter_id UUID REFERENCES users(id) ON DELETE CASCADE,
  element_type VARCHAR(20) NOT NULL,  -- e.g., 'user', 'profile', or 'wallpaper'
  element_id UUID NOT NULL,           -- the ID of the reported user / profile / wallpaper
  reason TEXT NOT NULL,
  created_at TIMESTAMP DEFAULT NOW()
);

-- Table for tracking each wallpaper view
CREATE TABLE wallpaper_views (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  user_id UUID NOT NULL,
  count INTEGER DEFAULT 1,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE UNIQUE INDEX idx_wallpaper_views_unique ON wallpaper_views(wallpaper_id, user_id);

-- Table for tracking each wallpaper download
CREATE TABLE wallpaper_downloads (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  user_id UUID NOT NULL,
  count INTEGER DEFAULT 1,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE UNIQUE INDEX idx_wallpaper_downloads_unique ON wallpaper_downloads(wallpaper_id, user_id);

-- Table for tracking each wallpaper share
CREATE TABLE wallpaper_shares (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  user_id UUID NOT NULL,
  count INTEGER DEFAULT 1,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE UNIQUE INDEX idx_wallpaper_shares_unique ON wallpaper_shares(wallpaper_id, user_id);

-- Table for tracking each wallpaper report
CREATE TABLE not_approved_images (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  image_url TEXT NOT NULL,
  reason TEXT,
  created_at TIMESTAMP DEFAULT NOW()
);

CREATE TABLE pending_wallpapers (
  id SERIAL PRIMARY KEY,
  user_id VARCHAR(255) NOT NULL,
  title VARCHAR(100) NOT NULL,
  description VARCHAR(500),
  image_url TEXT NOT NULL,
  status VARCHAR(50) NOT NULL DEFAULT 'manual_approval',
  created_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE TABLE saved_wallpapers (
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  saved_at TIMESTAMP DEFAULT NOW(),
  PRIMARY KEY (user_id, wallpaper_id)
);

-- Table to store individual style entries.
CREATE TABLE styles (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  name VARCHAR(100) UNIQUE NOT NULL,
  created_at TIMESTAMP DEFAULT NOW()
);

-- Junction table for many-to-many relationship between wallpapers and styles.
CREATE TABLE wallpaper_styles (
  wallpaper_id UUID REFERENCES wallpapers(id) ON DELETE CASCADE,
  style_id UUID REFERENCES styles(id) ON DELETE CASCADE,
  PRIMARY KEY (wallpaper_id, style_id)
);

CREATE TABLE contact_messages (
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),
  user_id UUID REFERENCES users(id) ON DELETE CASCADE,
  name VARCHAR(255) NOT NULL,
  email VARCHAR(255) NOT NULL,
  subject VARCHAR(255) NOT NULL,
  message TEXT NOT NULL,
  created_at TIMESTAMP DEFAULT NOW()
);


CREATE TABLE blocked_ips (
    id SERIAL PRIMARY KEY,
    ip VARCHAR(45) NOT NULL,
    request_count INTEGER DEFAULT 0,
    first_request TIMESTAMP WITH TIME ZONE DEFAULT NOW(),
    last_request TIMESTAMP WITH TIME ZONE DEFAULT NOW(),
    block_start TIMESTAMP WITH TIME ZONE,
    block_end TIMESTAMP WITH TIME ZONE,
    route TEXT, -- (optional) The route that was being spamed.
    created_at TIMESTAMP WITH TIME ZONE DEFAULT NOW()
);

Done Tested ✅

verification req criteria 

Active Contributor: Regularly uploads high-quality wallpapers/art.
Follower Count: Has a certain number of followers (e.g., 1,000+).
Engagement Metrics: Art has a high number of downloads, likes, or shares.
Public Presence: Links to a portfolio, social profiles, or credentials.
Professional Artist Status: Working in graphic design, illustration, or wallpaper creation professionally.

